{
  "presets": ["module:react-native", "react-native", "babel-preset-expo", "module:react-native-dotenv"],
  "retainLines": true,
  "env": {
    "development": {
      "plugins": ["@babel/transform-react-jsx-source", "transform-react-jsx-source"]
    },
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}